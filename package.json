{
  "name": "react-ex",
  "type": "module",
  "engines": {
    "node": ">=24.6.0"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "check": "prettier --check . && eslint .",
    "clean": "rimraf dist *.tsbuildinfo vite.config.js vite.config.d.ts coverage",
    "prepare": "husky",
    "reset": "npm run clean && rimraf node_modules package-lock.json .husky/_ && npm install"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^6.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
